class Writer {
	init(prefix) {
		this.prefix = prefix;
	}

	write(val) {
		print(this.prefix + ": " + val);
	}
}

class Logger < Writer {
	init() {
		super.init("logger");
	}
}

fun write(klass, val) {
	var instance = klass();

	instance.write(val);
}

var klass = Logger;
write(klass, "hello"); // logger: hello
